<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/white" />
            <size
                android:width="50dp"
                android:height="50dp" />
            <stroke
                android:width="1dp"
                android:color="@color/red_bg" />
        </shape>
    </item>
    <item
        android:bottom="50dp"
        android:left="50dp"
        android:right="50dp"
        android:top="50dp">
        <shape  android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/colorPrimary" />
            <size
                android:width="50dp"
                android:height="50dp" />
            <stroke
                android:width="1dp"
                android:color="@color/blak" />
        </shape>
    </item>
</layer-list>