<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include layout="@layout/smartrefresh_layout" />

        <com.monitor.changtian.widght.CustomFAB xmlns:custom="http://schemas.android.com/apk/res-auto"
            android:id="@+id/cf_imageButton"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="center|right|bottom"
            android:layout_marginBottom="50dp"
            android:layout_marginRight="16dp"
            android:src="@drawable/ic_format_align_left_black_24dp"
            android:tint="@android:color/white"
            custom:bg_color="@color/colorPrimary" />
    </FrameLayout>
</LinearLayout>