<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.monitor.leader.LeaderMainActivity">


    <FrameLayout
        android:id="@+id/fl_leader"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/bottom_bar_leader"></FrameLayout>

    <com.monitor.changtian.widght.BottomBar_Leader
        android:id="@+id/bottom_bar_leader"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:background="#FFFFFF" />
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/bottom_bar_leader"
        android:background="@mipmap/comui_bar_top_shadow"/>

</RelativeLayout>
